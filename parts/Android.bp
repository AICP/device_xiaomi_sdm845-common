android_app {
    name: "XiaomiParts",

    srcs: ["src/**/*.java"],
    resource_dirs: ["res"],
    certificate: "platform",
    platform_apis: true,
    privileged: true,

    static_libs: [
        "com.aicp.settings.resources",
        "androidx.core_core",
        "androidx.preference_preference",
        "AicpGear-preference",
        "AicpGear-util",
    ],

    optimize: {
        proguard_flags_files: ["proguard.flags"],
    },
}
